{
    "name": "@sellerspot/universal-types",
    "version": "1.8.7",
    "description": "Contains all common type definitions shared across all applications in the SellerSpot ecosystem.",
    "main": "dist/index.js",
    "scripts": {
        "dev": "tsc-watch --onSuccess 'yalc push --no-scripts'",
        "build": "webpack --mode production --env production",
        "build:dev": "webpack --mode development --env production && yalc push",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "yalc:link": "",
        "yalc:remove": "yalc remove --all",
        "prettier": "prettier --write src",
        "lint-staged": "lint-staged",
        "husky:install": "husky install",
        "husky:pre-commit": "npm run lint-staged",
        "prepare": "npm run husky:install",
        "update:pacakges": "npm i -D @sellerspot/webpack-run-scripts-custom-plugin"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SellerSpot/universal-types.git"
    },
    "keywords": [
        "sellerspot",
        "universal-types"
    ],
    "author": "Sellerspot",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/SellerSpot/universal-types/issues"
    },
    "homepage": "https://github.com/SellerSpot/universal-types#readme",
    "devDependencies": {
        "@sellerspot/webpack-run-scripts-custom-plugin": "^1.17.3",
        "@types/mongoose": "^5.10.4",
        "@types/webpack": "^4.41.25",
        "@types/webpack-node-externals": "^2.5.0",
        "@typescript-eslint/eslint-plugin": "^4.10.0",
        "@typescript-eslint/parser": "^4.10.0",
        "clean-webpack-plugin": "^3.0.0",
        "eslint": "^7.16.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.3.0",
        "fork-ts-checker-webpack-plugin": "^6.0.7",
        "husky": "^6.0.0",
        "lint-staged": "^10.5.4",
        "prettier": "^2.2.1",
        "ts-loader": "^8.0.12",
        "ts-node": "^9.1.1",
        "tsc-watch": "^4.2.9",
        "typescript": "^4.1.3",
        "webpack": "^4.46.0",
        "webpack-cli": "^4.2.0",
        "webpack-node-externals": "^2.5.2",
        "yalc": "^1.0.0-pre.50"
    }
}
